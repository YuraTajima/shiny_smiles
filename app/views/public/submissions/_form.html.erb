<%= form_with model:submission,local:true do |f| %>
    <div id="star">
      <%= f.label :star, '評価' %>
      <%= f.hidden_field :star, id: :review_star %>
    </div>
      <script>
　　　　$('#star').empty();
        $('#star').raty({
          size     : 36,
          starOff:  '<%= asset_path('star-off.png') %>',
          starOn : '<%= asset_path('star-on.png') %>',
 　 　　  　scoreName:sub 'review[star]',
          half: false,
        });
      </script>
      
<% end %>