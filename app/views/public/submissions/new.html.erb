<main>
    
     <div class="">
        <h4>カテゴリ</h4>
        <ul>
            <li>歯ブラシ</li>
            <li>歯磨き粉</li>
            <li>補助的清掃用具</li>
            <li>洗口液</li>
            <li>ホワイトニング用品</li>
            <li>口臭ケア用品</li>
        </ul>
    </div>
    
    <h3>投稿機能</h3>
    
    <div>
        <%= form_with model: @submissiont, url: '/submissions', method: :post do |f| %>
        <form>
            <ul>
                <li class="">
                    <label for="">画像<label>
                </li>
                <li class="">
                    <label for="">商品名</label>
                    <input><%= f.text_field :name %>
                </li>
                <li class="">
                    <label for="">ジャンル</label>
                </li>
                <li class="">
                    <label for="">値段</label>
                </li>
                <li class="">
                    <label for="">説明</label>
                </li>
                <li>
                    <input id="button" type="submit" name="button" value="投稿する">
                </li>
            </ul>
        </form>
        <% end %>
    </div>
    
</main>