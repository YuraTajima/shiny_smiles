<main>
    
    <div>
        <h3>投稿履歴</h3>
    </div>
    
    <div>
      <% @submissions.each do |submission| %>
        <ul>
            <li>
                <%= image_tag submission.get_image(400,400) %>
            </li>
            <li>
                <label for="">商品名</label>
                <%= submission.name %>
            </li>
            <li>
                <%= submission.created_at.strftime('%Y/%m/%d') %>
            </li>
        </ul>
      <% end %>
    </div>
</main>